#!/bin/bash

python main.py --SDE_Loss Bridge_rKL_logderiv --Energy_Config StudentTMixture --n_integration_steps 128 --T_start 1.0 --T_end 1. --batch_size 2000 --Interpol_lr 0.01 --N_anneal 6000 --feature_dim 64 --n_hidden 64 --GPU 4 --beta_min 0.01 --use_interpol_gradient --Network_Type FeedForward --project_name experiment_runs --use_normal --SDE_Type Bridge_SDE --n_particles 50 --clip_value 1 --lr 0.0001 --SDE_lr 0.0001 --sigma_init 20. --beta_max 2.5 --base_net PISgradnet --model_seeds 0 1 2 3 --sample_seed 42 43 44 45 --epochs_per_eval 100

python main.py --SDE_Loss Bridge_rKL_logderiv --Energy_Config Funnel --n_integration_steps 128 --T_start 1.0 --T_end 1. --batch_size 2000 --lr 0.005 --Energy_lr 0.0 --SDE_lr 0.005 --N_anneal 6000 --feature_dim 64 --n_hidden 64 --GPU 4 --beta_max 0.1 --beta_min 0.01 --use_interpol_gradient --Network_Type FeedForward --project_name experiment_runs --use_normal --SDE_Type Bridge_SDE --repulsion_strength 0.0 --sigma_init 1. --n_particles 10 --Interpol_lr 0.01 --model_seeds 0 1 2 3 --sample_seed 42 43 44 45 --epochs_per_eval 100